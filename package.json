{
  "name": "cm-web",
  "version": "1.0.0",
  "description": "Chris Miller website",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watch",
    "dev": "node server",
    "build": "next build",
    "start": "NODE_ENV=production node server",
    "heroku-postbuild": "heroku-nextjs-build",
    "lint": "eslint components layouts pages tests --fix",
    "generate": "DEBUG=Generator node bin/make.js",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --no-semi --write '{layouts,modules,hoc,components,utils,store,pages}/*.js'"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "yarn prettier", "git add"]
  },
  "repository": "git://github.com/chrismllr/cm-web.git",
  "keywords": [
    "chris",
    "miller",
    "front-end",
    "web",
    "developer",
    "portfolio"
  ],
  "author": "Chris Miller <chrsmllr@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@mars/heroku-nextjs-build": "^1.1.0",
    "axios": "^0.15.3",
    "date-fns": "^1.27.2",
    "express": "^4.15.0",
    "next": "next",
    "next-redux-wrapper": "^1.0.0",
    "normalizr": "^3.2.2",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.0.0",
    "sw-precache-webpack-plugin": "^0.9.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "debug": "^2.6.8",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "husky": "^0.14.3",
    "jest": "19.0.0",
    "jest-cli": "^19.0.2",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2"
  },
  "cacheDirectories": [
    "nextjs/node_modules"
  ],
  "jest": {
    "browser": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
